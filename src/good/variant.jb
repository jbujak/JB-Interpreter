variant {
	int num;
	string str;
} my_variant;

void main() {
	my_variant var1 = :num(1);
	my_variant var2 = :str("a");

	var n = 0;

	string s = "";
	s = case var1 of
		| :num(n) -> ":num(" + int_to_string(n) + ")"
		| :str(s) -> (":str(" + s + ")");
	print(s);

	s = case var2 of
		| :num(n) -> ":num(" + int_to_string(n) + ")"
		| :str(s) -> ":str(" + s + ")";
	print(s);
	print(int_to_string(n));
}
